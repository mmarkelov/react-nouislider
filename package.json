{
  "name": "nouislider-react",
  "private": false,
  "version": "1.3.1",
  "description": "React component wrapping leongersen/noUiSlider",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src -d build",
    "test": "jest ./tests/",
    "test:coverage": "jest --coverage ./tests/",
    "test:watch": "jest --watch ./tests/",
    "prepublish": "npm run build",
    "prettier": "prettier --write src/*.js",
    "precommit": "lint-staged"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setupJest.js"
    ]
  },
  "lint-staged": {
    "src/*.js": [
      "npm run prettier",
      "git add"
    ],
    "*.md": [
      "npm run prettier",
      "git add"
    ]
  },
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mmarkelov/react-nouislider"
  },
  "keywords": [
    "react",
    "nouislider",
    "slider",
    "component",
    "reactjs",
    "range",
    "slider"
  ],
  "author": "Maksim Markelov <mapkeji26@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "nouislider": "^11.1.0",
    "react": "^16.4.1"
  },
  "peerDependencies": {
    "nouislider": "11.x",
    "react": "16.3.x || 16.4.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "prop-types": "^15.6.2",
    "react-dom": "^16.4.1"
  }
}
